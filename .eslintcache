l = setInterval(() => {\r\n        if (seconds > 0) {\r\n          setSeconds(seconds - 1);\r\n        }\r\n        if (seconds === 0) {\r\n          if (minutes === 0) {\r\n            clearInterval(myInterval);\r\n          } else {\r\n            setMinutes(minutes - 1);\r\n            setSeconds(59);\r\n          }\r\n        }\r\n        setProgress(300 - percent);\r\n      }, 1000);\r\n\r\n      return () => {\r\n        clearInterval(myInterval);\r\n      };\r\n    }\r\n  });\r\n\r\n  return (\r\n    <div className={style.timer}>\r\n      <div className={style.background}></div>\r\n      <div className={style.clock}>\r\n        {minutes === 0 && seconds === 0 ? null : (\r\n          <div>\r\n            {\" \"}\r\n            {minutes}:{seconds < 10 ? `0${seconds}` : seconds}\r\n          </div>\r\n        )}\r\n      </div>\r\n\r\n      <div className={style.progressRing}>\r\n        <svg height={radius * 2} width={radius * 2}>\r\n          <circle\r\n            stroke=\"#f87070\"\r\n            fill=\"transparent\"\r\n            strokeWidth={stroke}\r\n            strokeDasharray={circumference + \" \" + circumference}\r\n            style={{ strokeDashoffset }}\r\n            stroke-width={stroke}\r\n            r={normalizedRadius}\r\n            cx={radius}\r\n            cy={radius}\r\n          />\r\n        </svg>\r\n      </div>\r\n\r\n      <button\r\n        className={style.btn}\r\n        onClick={() => {\r\n          setIsTimerOn(!isTimerOn);\r\n        }}\r\n      >\r\n        {isTimerOn ? \"PAUSE\" : \"START\"}\r\n      </button>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Timer;\r\n","C:\\Users\\parej\\Desktop\\code2021\\potsredux\\src\\components\\TimerSelector\\index.js",[],"C:\\Users\\parej\\Desktop\\code2021\\potsredux\\src\\components\\TimerSettings\\index.js",[],"C:\\Users\\parej\\Desktop\\code2021\\potsredux\\src\\features\\timer\\timerSlice.js",[],"C:\\Users\\parej\\Desktop\\code2021\\potsredux\\src\\features\\theme\\themeSlic